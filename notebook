    "# Create dummy variables\n",
    "category = df.nunique()[df.nunique() < 10].keys().tolist()\n",
    "df_cat = df[category]\n",
    "df_cat = pd.get_dummies(df_cat)\n",
    "df_cat.head()"
   ]
