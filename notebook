     "execution_count": 160,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "df_engin['SupposedCharge'] = df_engin['tenure'] * df_engin['MonthlyCharges']\n",
    "df_engin['ChangeContract'] = df_engin['SupposedCharge'] - df_engin['TotalCharges']\n",
    "# May change contract if the difference is larger than $100, otherwise maybe accounting error\n",
    "df_engin['ChangeContract'] = df_engin['ChangeContract'].map(lambda x: 1 if abs(x) > 100 else 0) \n",
    "\n",
    "df_engin.head()"
   ]
